#include <iostream>
#include <ctime>
using namespace std;
int main()
{
	int n;
	setlocale(LC_ALL, "Russian");
	srand(time(NULL));
	cout << "введите количество попыток" << endl;
	cin >> n;
	while (n > 0)
	{
		int kos1=0, kos2=0;
		int sum=0;
		kos1 = 1+rand() % 5+1;
		kos2 = 1+rand() % 5+1;
		sum = kos1 + kos2;
		cout << sum << endl;
		sum = 0;
		n-=1;
	}
}
