#include <iostream>
using namespace std;

int del(int n)
{
	int sum = 0;
	for (int i = 1; i <= ((n / 2)+1); i++)
	{
		if (n % i == 0)
		{
			sum += i;
		}
	}

	return sum;
}

int main()
{

	for (int i = 2; i < 10000; i++)
	{ 
		if (i > del(i)) {
			if (i == del(del(i)))
			{
				cout << i << " - " << del(i) << endl;
			}
		}
		
	}
}
